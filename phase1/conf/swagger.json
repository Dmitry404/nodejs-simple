{
  "info": {
    "title": "Node.js course : Phase 1 : Swagger",
    "description": "A bit more complicated sample project",
    "version": "0.1.0"
  },
  "host": "localhost:3000",
  "basePath": "/api/v1",
  "paths": {
    "/readme": {
      "get": {
        "description": "Welcomes user. The only available API in 0.1.0",
        "produces": [
          "text/markdown"
        ],
        "responses": {
          "200": {
            "description": "I'm fine"
          },
          "304": {
            "description": "I'm still fine"
          }
        }
      }
    }
  }
}
